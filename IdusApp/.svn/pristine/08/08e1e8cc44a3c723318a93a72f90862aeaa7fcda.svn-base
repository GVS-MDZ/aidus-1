package com.gvs.utilidades;

public class OperationClient {
private String name;
private int icon;

public OperationClient(String name, int icon) {
	super();
	this.name = name;
	this.icon = icon;
}
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getIcon() {
	return icon;
}
public void setIcon(int icon) {
	this.icon = icon;
}

}
